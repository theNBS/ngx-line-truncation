{
  "name": "@nbs/ngx-line-truncation",
  "version": "1.9.2",
  "description": "Ngx Line Truncation is line truncation implementation for Angular that truncate text by user defined line number.",
  "scripts": {
    "post-nbs-publish": "sed -i 's/@nbs:registry=https:/@nbs:registry=http:/g' ~/.npmrc && npm run update-version-history-reminder --silent",
    "pre-nbs-publish": "sed -i 's/@nbs:registry=http:/@nbs:registry=https:/g' ~/.npmrc",
    "nbs-publish": "npm run pre-nbs-publish && npm publish && npm run post-nbs-publish"
  },
  "keywords": [
    "ellipsis",
    "truncation",
    "truncate",
    "truncated",
    "shave",
    "js",
    "content",
    "line clamp",
    "javascript",
    "shorten",
    "trim"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/DiZhou92/ngx-line-truncation"
  },
  "peerDependencies": {
    "@angular/common": ">=10",
    "@angular/core": ">=10"
  },
  "dependencies": {
    "line-truncation": "^1.3.9",
    "tslib": "^2.0.0"
  }
}
